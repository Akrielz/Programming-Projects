            <kind>1</kind>
            <string>///Player's Code


//Pause Code
if keyboard_check_pressed(ord("P"))
    global.Pause = !global.Pause;
    
//Mute Code
if keyboard_check_pressed(ord("M"))
    global.Mute *= -1;

if global.Mute == 1 and global.Trigger_Mute == 0
{
    global.Trigger_Mute = 1;
    global.Trigger_Not_Mute = 0;
    global.Volume = 0;
    audio_sound_gain(snd_ChaosFantasy, global.Volume, 2000);
}
  
if global.Pause
{
    if global.Mute == -1
        audio_pause_sound(snd_ChaosFantasy);
}
else
{
    audio_resume_sound(snd_ChaosFantasy);
    if global.Mute == -1 and global.Trigger_Not_Mute == 0
    {
        global.Trigger_Mute = 0;
        global.Trigger_Not_Mute = 1;
        global.Volume = 1;
        audio_sound_gain(snd_ChaosFantasy, global.Volume, 2000);
    }

    //Rotating System
    if ( place_meeting ( x, y+1, obj_platform ) )
        image_angle = 0;
    else
        image_angle -= 5;
    
    //Getting the Imputs
    Key_Jump = keyboard_check( vk_space );

    if ( keyboard_check ( vk_space ) )
        Active = 1;
    if ( Vertical_Speed &lt; 30 )
        Vertical_Speed += My_Gravity;
        
    if ( place_meeting( x, y+1, obj_platform ) )
    {
        Ground = 1;
        if ( Ground == 1 and Active )
        {
            Vertical_Speed = Key_Jump * -Jump_Speed;
                Active = 0;
        }
    }
    else
        Ground = 0;
    
    x += Move_Speed;
    obj_camera.x += Move_Speed
    
    if ( place_meeting( x, y + Vertical_Speed, obj_platform ) )
    {
        while ( !place_meeting ( x, y + sign ( Vertical_Speed ), obj_platform ) )
        {
            y += sign ( Vertical_Speed );
            obj_camera.y += sign ( Vertical_Speed );
        }
        Vertical_Speed = 0;
    }
    y += Vertical_Speed;
    obj_camera.y += Vertical_Speed;
}
</string>
